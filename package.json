{
  "name": "lubejs-mssql",
  "version": "3.0.0-preview02",
  "description": "The lubejs driver for mssql.",
  "main": "dist/index.js",
  "directories": {
    "dist": "dist"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && tsc && npm pack",
    "clean": "pwsh -c 'rm -r -force dist'",
    "lint": "eslint --fix ./src --ext .ts"
  },
  "keywords": [
    "mssql",
    "lubejs",
    "lube",
    "js2sql",
    "sql",
    "sqlserver",
    "mssqlserver"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "author": "jover",
  "license": "MIT",
  "dependencies": {
    "@jovercao/mssql": "^7.2.1-jover.2",
    "core-decorators": "^0.20.0"
  },
  "devDependencies": {
    "@types/mssql": "^6.0.8",
    "@types/node": "^14.17.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.4.1",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
