{
  "name": "lubejs-mssql",
  "version": "3.0.0-preview02",
  "description": "The lubejs driver for mssql.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc && npm run pack",
    "clean": "pwsh -c if (test-path dist) { rm -r -force dist }",
    "pack": "pwsh -c cp -r typings dist/; cp README.md dist/; cp package.json dist/; cd dist; npm pack;",
    "lint": "eslint --fix ./src --ext .ts"
  },
  "keywords": [
    "mssql",
    "lubejs",
    "lube",
    "js2sql",
    "sql",
    "sqlserver",
    "mssqlserver"
  ],
  "author": "jover",
  "license": "MIT",
  "dependencies": {
    "core-decorators": "^0.20.0"
  },
  "devDependencies": {
    "@jovercao/mssql": "^7.2.1-jover.4",
    "@types/mssql": "^6.0.8",
    "@types/node": "^14.17.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.32.0",
    "typescript": "^4.3.5"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
